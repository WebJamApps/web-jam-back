{
  "name": "web-jam-back",
  "version": "0.8.4",
  "description": "web-jam.com",
  "main": "index.js",
  "engines": {
    "node": ">=12.5.0"
  },
  "files": [
    "lib/",
    "model/",
    "auth/",
    "config.js",
    "routes.js"
  ],
  "license": "MIT",
  "scripts": {
    "cc": "codeclimate analyze -f html > codeclimate.html",
    "start:debug": "DEBUG=web-jam-back:* nodemon index.js",
    "installglobals": "yarn config set registry https://registry.yarnpkg.com -g",
    "postinstall": "./postinstall.sh && ./postinstallJaM.sh",
    "dev": "nodemon index.js",
    "build:prod": "(cd frontend || exit; npm start -- build)",
    "start": "node index.js",
    "cleaninstall": "rm -rf yarn.lock && rm -rf node_modules && yarn cache clean && yarn installglobals && yarn install",
    "node:debug": "node --inspect-brk index.js",
    "test": "eslint . && rm -rf coverage && npm run test:jest && npm run test:mocha && npm run combine-coverage",
    "test:lint": "eslint . --fix",
    "test:local": "eslint . --fix && yarn test:unit && yarn cc",
    "test:mocha": "nyc mocha --timeout 6000 --bail -r ./test/mocha/bootstrap.js test/mocha/**/*.js --exit",
    "test:debug": "./node_modules/.bin/mocha --inspect-brk -r ./test/mocha/bootstrap.js test/mocha/**/*.js",
    "test:jest": "cross-env NODE_ENV=test && jest --no-cache --config jest.config.json --notify",
    "test:unit": "rm -rf coverage && yarn test:jest && yarn test:mocha && yarn combine-coverage",
    "combine-coverage": "istanbul-combine -d coverage -p none -r lcov -r html coverage/mocha/coverage-final.json coverage/jest/coverage-final.json",
    "snyk-protect": "snyk protect"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/WebJamApps/web-jam-back.git"
  },
  "author": "HandlersAndKickers <>",
  "dependencies": {
    "@sendgrid/mail": "latest",
    "bcryptjs": "latest",
    "bluebird": "latest",
    "body-parser": "latest",
    "cross-env": "latest",
    "cors": "latest",
    "debug": "latest",
    "dotenv": "latest",
    "express": "latest",
    "express-sslify": "latest",
    "helmet": "latest",
    "jwt-simple": "latest",
    "lodash": "latest",
    "moment": "latest",
    "mongoose": "latest",
    "morgan": "latest",
    "nodemon": "latest",
    "request": "latest",
    "request-promise": "latest",
    "snyk": "^1.190.0"
  },
  "devDependencies": {
    "chai": "latest",
    "chai-http": "latest",
    "eslint": "latest",
    "eslint-config-airbnb": "latest",
    "eslint-plugin-import": "latest",
    "eslint-plugin-jest": "latest",
    "eslint-plugin-jsx-a11y": "latest",
    "eslint-plugin-node": "latest",
    "eslint-plugin-react": "latest",
    "eslint-plugin-security": "latest",
    "eslint-plugin-mocha": "latest",
    "jest": "latest",
    "jest-cli": "latest",
    "istanbul": "latest",
    "istanbul-combine": "latest",
    "mocha": "latest",
    "nock": "latest",
    "nodemon": "latest",
    "nyc": "latest",
    "sinon": "latest",
    "sinon-mongoose": "latest",
    "supertest": "latest"
  },
  "snyk": true
}