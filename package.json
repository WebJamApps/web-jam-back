{
  "name": "web-jam-back",
  "version": "0.8.0",
  "description": "web-jam.com",
  "main": "index.js",
  "engines": {
    "node": "10.9.0"
  },
  "files": ["lib/", "model/", "auth/", "config.js", "routes.js"],
  "license": "MIT",
  "scripts": {
    "installglobals": "npm install -g npm@latest",
    "postinstall": "./postinstall.sh",
    "dev": "nodemon index.js",
    "build:prod": "(cd frontend || exit; npm start -- build)",
    "start": "node index.js",
    "cleaninstall": "rm -rf yarn.lock && rm -rf node_modules && yarn cache clean && npm run installglobals && yarn install",
    "debug": "node --inspect-brk index.js",
    "test": "rm -rf coverage && npm run test:jest &&  npm run test:mocha && eslint .",
    "test:mocha": "nyc ./node_modules/.bin/_mocha --timeout 6000 -r ./test/bootstrap.js test/**/*.js --exit",
    "test:debug": "./node_modules/.bin/mocha --inspect-brk -r ./test/bootstrap.js test/**/*.js",
    "test:jest": "cross-env NODE_ENV=test && jest --no-cache --config jest.config.json --notify",
    "fix:lint": "./node_modules/eslint/bin/eslint.js --fix .",
    "snyk-protect": "snyk protect",
    "prepublish": "npm run snyk-protect"
  },
  "nyc": {
    "exclude": ["**/*.spec.js", "test"]
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/WebJamApps/web-jam-back.git"
  },
  "author": "HandlersAndKickers <>",
  "dependencies": {
    "@sendgrid/mail": "latest",
    "bcryptjs": "^2.4.3",
    "bluebird": "^3.5.1",
    "body-parser": "^1.17.1",
    "cross-env": "latest",
    "cors": "^2.8.1",
    "dotenv": "latest",
    "express": "latest",
    "express-sslify": "^1.2.0",
    "helmet": "^3.6.1",
    "jwt-simple": "^0.5.0",
    "lodash": "^4.15.0",
    "moment": "^2.15.2",
    "mongoose": "latest",
    "morgan": "^1.8.1",
    "request": "latest",
    "request-promise": "latest",
    "snyk": "latest"
  },
  "devDependencies": {
    "chai": "latest",
    "chai-http": "latest",
    "eslint": "latest",
    "eslint-config-airbnb": "latest",
    "eslint-plugin-import": "latest",
    "eslint-plugin-jsx-a11y": "latest",
    "eslint-plugin-node": "latest",
    "eslint-plugin-react": "latest",
    "eslint-plugin-security": "latest",
    "jest": "latest",
    "jest-cli": "latest",
    "istanbul": "latest",
    "mocha": "latest",
    "nock": "latest",
    "nodemon": "latest",
    "nyc": "latest",
    "sinon": "latest",
    "sinon-mongoose": "latest"
  },
  "snyk": true
}
