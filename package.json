{
  "name": "web-jam-back",
  "version": "1.6.0",
  "description": "web-jam.com",
  "main": "index.js",
  "engines": {
    "node": ">=18.12.0 <19",
    "npm": ">=8.19.2 <9"
  },
  "files": [
    "lib/",
    "model/",
    "auth/",
    "config.js",
    "routes.js"
  ],
  "license": "MIT",
  "scripts": {
    "rmrf": "rm -rf build && rm -rf JaMmusic && rm -rf coverage",
    "start:debug": "rm -rf build && tsc && npm-run-all -p ts-start ts-watch",
    "ts-watch": "tsc -w",
    "cc": "codeclimate analyze -f html > codeclimate.html",
    "ts-start": "DEBUG=web-jam-back:* nodemon build/index.js",
    "installglobals": "npm install -g npm@latest && npm install -g snyk",
    "build:front": "./postinstallJaM.sh && rm -rf ./JaMmusic/src && rm -rf ./JaMmusic/test",
    "postinstall": "npm run rmrf && tsc && npm run build:front",
    "start": "node build/index.js",
    "cleaninstall": "rm -rf yarn.lock && rm -rf node_modules && rm -rf package-lock.json && npm install -g npm@latest && npm run rmrf && npm cache clean --force && npm install",
    "node:debug": "node --inspect-brk index.js",
    "test": "eslint ./src && rm -rf coverage && npm run test:jest",
    "test:lint": "eslint ./src --fix",
    "test:local": "npm run test:lint && rm -rf coverage && npm run test:jest && npm run cc",
    "test:jest": "jest --no-cache --config jest.config.json --runInBand --forceExit"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/WebJamApps/web-jam-back.git"
  },
  "author": "HandlersAndKickers <>",
  "dependencies": {
    "@sendgrid/mail": "^7.7.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.12",
    "@types/debug": "^4.1.7",
    "@types/express": "^4.17.14",
    "@types/express-sslify": "^1.2.2",
    "@types/morgan": "^1.9.3",
    "@types/superagent": "^4.1.15",
    "@types/supports-color": "^8.1.1",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "csvtojson": "^2.0.10",
    "debug": "^4.3.4",
    "dotenv": "^16.0.2",
    "express": "^4.18.1",
    "express-sslify": "^1.2.0",
    "helmet": "^5.0.2",
    "jwt-simple": "^0.5.6",
    "moment": "^2.29.4",
    "mongoose": "^6.5.0",
    "morgan": "1.10.0",
    "snyk": "^1.996.0",
    "superagent": "^8.0.0",
    "supports-color": "^8.1.1",
    "typescript": "^4.8.3"
  },
  "devDependencies": {
    "@types/jest": "^29.0.3",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.37.0",
    "@typescript-eslint/parser": "^5.37.0",
    "eslint": "^8.23.1",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-plugin-es": "^4.1.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^27.0.4",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-security": "^1.5.0",
    "jest": "^29.0.3",
    "nodemon": "^2.0.19",
    "npm-run-all": "^4.1.5",
    "supertest": "^6.2.4",
    "ts-jest": "^29.0.1"
  }
}
