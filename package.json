{
  "name": "web-jam-back",
  "version": "1.9.0",
  "description": "web-jam.com",
  "main": "index.js",
  "engines": {
    "node": ">=20.9.0 <21",
    "npm": ">=10.2.1 <11"
  },
  "files": [
    "src/"
  ],
  "license": "MIT",
  "scripts": {
    "rmrf": "rm -rf build && rm -rf JaMmusic && rm -rf coverage",
    "start:debug": "rimraf build && tsc && npm-run-all -p ts-start ts-watch",
    "ts-watch": "tsc -w",
    "cc": "codeclimate analyze -f html > codeclimate.html",
    "ts-start": "cross-env DEBUG=web-jam-back:* nodemon --trace-deprecation build/index.js",
    "installglobals": "npm install -g npm@latest && npm install -g snyk",
    "build:front": "./postinstallJaM.sh && rm -rf ./JaMmusic/src && rm -rf ./JaMmusic/test",
    "postinstall": "npm run rmrf && tsc && npm run build:front",
    "start": "node build/index.js",
    "cleaninstall": "rm -rf yarn.lock && rm -rf node_modules && rm -rf package-lock.json && npm install -g npm@latest && npm run rmrf && npm cache clean --force && npm install",
    "node:debug": "node --inspect-brk index.js",
    "test": "eslint ./src && rimraf coverage && npm run test:jest",
    "test:lint": "eslint ./src --fix",
    "test:local": "npm run test:lint && rm -rf coverage && npm run test:jest && npm run cc",
    "test:jest": "jest --no-cache --config jest.config.json --runInBand --forceExit"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/WebJamApps/web-jam-back.git"
  },
  "author": "HandlersAndKickers <>",
  "dependencies": {
    "@sendgrid/mail": "^7.7.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.13",
    "@types/debug": "^4.1.7",
    "@types/express": "^4.17.17",
    "@types/express-sslify": "^1.2.2",
    "@types/morgan": "^1.9.4",
    "@types/superagent": "^4.1.16",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "crossenv": "^0.0.2-security",
    "csvtojson": "^2.0.10",
    "debug": "^4.3.4",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-sslify": "^1.2.0",
    "helmet": "^6.0.1",
    "jwt-simple": "0.5.6",
    "moment": "2.29.4",
    "mongoose": "8.0.3",
    "morgan": "1.10.0",
    "rimraf": "^5.0.5",
    "snyk": "^1.1118.0",
    "superagent": "^8.1.2",
    "typescript": "^5.2.2"
  },
  "devDependencies": {
    "@types/jest": "^29.5.9",
    "@types/supertest": "^2.0.16",
    "@typescript-eslint/eslint-plugin": "^6.11.0",
    "@typescript-eslint/parser": "^6.11.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.53.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-plugin-es": "^4.1.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-jest": "^27.6.0",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-security": "^1.7.1",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "npm-run-all": "^4.1.5",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1"
  },
  "snyk": true
}
