{
  "name": "web-jam-back",
  "version": "0.8.2",
  "description": "web-jam.com",
  "main": "index.js",
  "engines": {
    "node": ">=11.9.0"
  },
  "files": ["lib/", "model/", "auth/", "config.js", "routes.js"],
  "license": "MIT",
  "scripts": {
    "cc": "codeclimate analyze -f html > codeclimate.html",
    "installglobals": "npm install -g npm@latest && npm install -g yarn@latest",
    "postinstall": "./postinstall.sh",
    "dev": "nodemon index.js",
    "build:prod": "(cd frontend || exit; npm start -- build)",
    "start": "node index.js",
    "cleaninstall": "rm -rf yarn.lock && rm -rf node_modules && yarn cache clean && npm run installglobals && yarn install",
    "debug": "node --inspect-brk index.js",
    "test": "rm -rf coverage && npm run test:jest && npm run test:mocha && npm run combine-coverage && eslint .",
    "test:mocha": "nyc mocha --timeout 6000 --bail -r ./test/mocha/bootstrap.js test/mocha/**/*.js --exit",
    "test:debug": "./node_modules/.bin/mocha --inspect-brk -r ./test/mocha/bootstrap.js test/mocha/**/*.js",
    "test:jest": "cross-env NODE_ENV=test && jest --no-cache --config jest.config.json --notify",
    "combine-coverage": "istanbul-combine -d coverage -p none -r lcov -r html coverage/mocha/coverage-final.json coverage/jest/coverage-final.json",
    "fix:lint": "./node_modules/eslint/bin/eslint.js --fix .",
    "snyk-protect": "snyk protect"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/WebJamApps/web-jam-back.git"
  },
  "author": "HandlersAndKickers <>",
  "dependencies": {
    "@sendgrid/mail": "latest",
    "bcryptjs": "latest",
    "bluebird": "latest",
    "body-parser": "latest",
    "cross-env": "latest",
    "cors": "latest",
    "dotenv": "latest",
    "express": "latest",
    "express-sslify": "latest",
    "helmet": "latest",
    "jwt-simple": "latest",
    "lodash": "latest",
    "moment": "latest",
    "mongoose": "latest",
    "morgan": "latest",
    "request": "latest",
    "request-promise": "latest",
    "snyk": "1.124.1"
  },
  "devDependencies": {
    "chai": "latest",
    "chai-http": "latest",
    "eslint": "latest",
    "eslint-config-airbnb": "latest",
    "eslint-plugin-import": "latest",
    "eslint-plugin-jest": "latest",
    "eslint-plugin-jsx-a11y": "latest",
    "eslint-plugin-node": "latest",
    "eslint-plugin-react": "latest",
    "eslint-plugin-security": "latest",
    "jest": "latest",
    "jest-cli": "latest",
    "istanbul": "latest",
    "istanbul-combine": "latest",
    "mocha": "latest",
    "nock": "latest",
    "nodemon": "latest",
    "nyc": "latest",
    "sinon": "latest",
    "sinon-mongoose": "latest",
    "supertest": "latest"
  },
  "snyk": true
}
