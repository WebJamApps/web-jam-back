{
  "name": "web-jam-back",
  "version": "0.1.0",
  "description": "web-jam.com",
  "main": "index.js",
  "engines": {
    "node": "8.1.0",
    "npm": "5.0.3"
  },
  "license": "MIT",
  "scripts": {
    "preinstall": "npm install -g yarn",
    "postinstall": "./postinstall.sh",
    "dev": "nodemon index.js",
    "build:prod": "(cd frontend || exit; npm run vulc && npm start -- build)",
    "start": "node index.js",
    "cleaninstall": "rm -rf yarn.lock && rm -rf node_modules && npm install -g yarn && yarn cache clean && yarn install",
    "debug": "node --inspect-brk index.js",
    "mocha": "mocha  -r ./test/bootstrap.js test/**/*.js",
    "nyc": "nyc npm run mocha",
    "test": "eslint . && istanbul  cover  ./node_modules/.bin/_mocha  -r ./test/bootstrap.js test/**/*.js  --coverage --dir ./coverage",
    "test:debug": "./node_modules/.bin/mocha --inspect-brk -r ./test/bootstrap.js test/**/*.js",
    "eslint-fix": "./node_modules/eslint/bin/eslint.js --fix .",
    "snyk-protect": "snyk protect"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/WebJamApps/web-jam-back.git"
  },
  "author": "HandlersAndKickers <>",
  "contributors": [
    {
      "name": ""
    },
    {
      "name": ""
    }
  ],
  "dependencies": {
    "bluebird": "^3.4.1",
    "body-parser": "^1.17.1",
    "cors": "^2.8.1",
    "dotenv": "^2.0.0",
    "express": "^4.15.2",
    "express-sslify": "^1.2.0",
    "helmet": "^3.6.1",
    "jwt-simple": "^0.5.0",
    "lodash": "^4.15.0",
    "moment": "^2.15.2",
    "mongoose": "^4.10.0",
    "morgan": "^1.8.1",
    "request": "^2.78.0",
    "snyk": "^1.30.1"
  },
  "devDependencies": {
    "babel-eslint": "^7.2.3",
    "chai": "^3.5.0",
    "chai-as-promised": "^6.0.0",
    "chai-http": "^3.0.0",
    "chokidar-cli": "^1.2.0",
    "eslint": "^3.2.2",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.0.0",
    "istanbul": "^0.4.5",
    "mocha": "^3.1.2",
    "mockgoose": "^6.0.8",
    "nock": "^9.0.13",
    "nodemon": "^1.9.2",
    "proxyquire": "^1.7.11",
    "sinon": "^1.17.6",
    "sinon-chai": "^2.8.0",
    "supertest": "^3.0.0"
  },
  "snyk": true
