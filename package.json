{
  "name": "web-jam-back",
  "version": "0.7.9",
  "description": "web-jam.com",
  "main": "index.js",
  "engines": {
    "node": "8.11.3"
  },
  "license": "MIT",
  "scripts": {
    "installglobals": "npm install -g npm@latest",
    "postinstall": "./postinstall.sh",
    "dev": "nodemon index.js",
    "build:prod": "(cd frontend || exit; npm start -- build)",
    "start": "node index.js",
    "cleaninstall": "rm -rf yarn.lock && rm -rf node_modules && yarn cache clean && npm run installglobals && yarn install",
    "debug": "node --inspect-brk index.js",
    "test": "rm -rf coverage && nyc --reporter=html --reporter=lcov ./node_modules/.bin/_mocha --timeout 6000 -r ./test/bootstrap.js test/**/*.js --exit && eslint .",
    "test:debug": "./node_modules/.bin/mocha --inspect-brk -r ./test/bootstrap.js test/**/*.js",
    "fix:lint": "./node_modules/eslint/bin/eslint.js --fix .",
    "snyk-protect": "snyk protect",
    "prepublish": "npm run snyk-protect"
  },
  "nyc": {
    "exclude": ["**/*.spec.js", "test"]
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/WebJamApps/web-jam-back.git"
  },
  "author": "HandlersAndKickers <>",
  "dependencies": {
    "@sendgrid/mail": "^6.2.1",
    "bcryptjs": "^2.4.3",
    "bluebird": "^3.4.1",
    "body-parser": "^1.17.1",
    "cors": "^2.8.1",
    "dotenv": "^2.0.0",
    "express": "^4.15.2",
    "express-sslify": "^1.2.0",
    "helmet": "^3.6.1",
    "jwt-simple": "^0.5.0",
    "lodash": "^4.15.0",
    "moment": "^2.15.2",
    "mongoose": "^5.1.1",
    "morgan": "^1.8.1",
    "request": "^2.78.0",
    "sinon-mongoose": "^2.1.1"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "chai-http": "^4.0.0",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-react": "^7.8.2",
    "eslint-plugin-security": "^1.4.0",
    "istanbul": "^0.4.5",
    "mocha": "^5.2.0",
    "nock": "^9.2.6",
    "nodemon": "^1.17.4",
    "nyc": "^12.0.2",
    "sinon": "^5.0.7",
    "snyk": "^1.80.1"
  },
  "snyk": true
}
