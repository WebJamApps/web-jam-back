{
  "name": "web-jam-back",
  "version": "0.8.7",
  "description": "web-jam.com",
  "main": "index.js",
  "engines": {
    "node": ">=13.0.1"
  },
  "files": [
    "lib/",
    "model/",
    "auth/",
    "config.js",
    "routes.js"
  ],
  "license": "MIT",
  "scripts": {
    "cc": "codeclimate analyze -f html > codeclimate.html",
    "start:debug": "DEBUG=web-jam-back:* nodemon index.js",
    "installglobals": "yarn global add npm@latest",
    "postinstall": "./postinstallJaM.sh",
    "dev": "nodemon index.js",
    "build:prod": "(cd JaMmusic || exit; npm start -- build)",
    "start": "node index.js",
    "cleaninstall": "rm -rf yarn.lock && rm -rf node_modules && yarn cache clean && yarn install",
    "node:debug": "node --inspect-brk index.js",
    "test": "eslint . && rm -rf coverage && npm run test:jest",
    "test:lint": "eslint . --fix",
    "test:local": "eslint . --fix && rm -rf coverage && yarn test:jest && yarn cc",
    "test:jest": "cross-env NODE_ENV=test && jest --no-cache --config jest.config.json --notify --detectOpenHandles --forceExit"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/WebJamApps/web-jam-back.git"
  },
  "author": "HandlersAndKickers <>",
  "dependencies": {
    "@sendgrid/mail": "latest",
    "bcryptjs": "latest",
    "bluebird": "latest",
    "body-parser": "latest",
    "cross-env": "latest",
    "cors": "latest",
    "debug": "latest",
    "dotenv": "latest",
    "express": "latest",
    "express-sslify": "latest",
    "helmet": "latest",
    "jwt-simple": "latest",
    "moment": "latest",
    "mongoose": "latest",
    "morgan": "latest",
    "nodemon": "latest",
    "superagent": "latest"
  },
  "devDependencies": {
    "eslint": "latest",
    "eslint-config-airbnb": "latest",
    "eslint-plugin-es": "latest",
    "eslint-plugin-import": "latest",
    "eslint-plugin-jest": "latest",
    "eslint-plugin-jsx-a11y": "latest",
    "eslint-plugin-node": "latest",
    "eslint-plugin-react": "latest",
    "eslint-plugin-react-hooks": "latest",
    "eslint-plugin-security": "latest",
    "jest": "latest",
    "nodemon": "latest",
    "supertest": "latest"
  }
}
